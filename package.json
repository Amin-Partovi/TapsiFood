{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "prettier:check": "prettier --check './src/**/*.{js,jsx,ts,tsx,json,md}'",
    "prettier:fix": "prettier --write './src/**/*.{js,jsx,ts,tsx,json,md}'",
    "generate:icons": "npx @svgr/cli --typescript --out-dir ./src/components/icons --ignore-existing -- ./src/assets/icons",
    "generate:illustrations": "npx @svgr/cli --typescript --out-dir ./src/components/illustrations --ignore-existing -- ./src/assets/illustrations"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
