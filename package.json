    "@tanstack/react-query-devtools": "^5.62.2",
    "@tanstack/react-table": "^8.20.5",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "json-server": "^1.0.0-beta.3",
    "next": "15.0.3",
    "postcss-nesting": "^13.0.1",
    "qs": "^6.13.1",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
